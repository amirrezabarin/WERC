
@using WERC.Models

@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Styles.Render("~/Resources/CSS/jquery-ui")

@section LabMenu{
    <!--Lab Menu start-->
    <nav class="navbar navbar-inverse" role="navigation">
        <div class="container-fluid menu-height">
            <div class="row">

                <div class="navbar-header head-foot-back-color">

                    <!--Menu Button-->
                    <button type="button" class="navbar-toggle" data-toggle="collapse"
                            data-target=".navbar-ex1-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <div class="partialLogin text-left visible-xs col-xs-6">
                        @Html.Partial("_LoginPartial",
                            new LoginPartialViewModel
                            {
                                LanguageDictionary = Model.LanguageDictionary,
                                Layout = Model.Layout,
                                WelcomeMessage = Model.WelcomeMessage
                            })
                    </div>
                </div>
                <!--Menu Items-->
                <div class="collapse navbar-collapse navbar-ex1-collapse head-foot-back-color">

                    <ul class="nav navbar-nav navbar-left col-sm-8 col-md-8">
                        <li class="">
                            <a href="~/Home">@Model["HOME"] </a>
                        </li>

                        <li class="">
                            <a href="~/lab/lttmf">@Model["TEAM TEST MANAGEMENT"] </a>
                        </li>
                        @*<li class="">
                            <a href="/grade/lgtf">@Model["GRADING"] </a>
                        </li>*@

                        <li class="dropdown">

                            <a href="./test" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                <span class="menuItemText">@Model["PROFILE"]<i class="caret"></i></span>
                            </a>

                            <ul class="dropdown-menu">

                                <li>
                                    <a href="/lab/lupf">
                                        <i class="fa fa-user-circle-o fa-1x" aria-hidden="true"></i>
                                        <span class="">@Model["Change Profile"] </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="" href="/Account/ForgotPassword?Length=7">
                                        <i class="fa fa-key fa-1x" aria-hidden="true"></i>
                                        <span class="">@Model["Reset Password"] </span>
                                    </a>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </div>
            </div>
        </div>
    </nav>
}
<div class="col-xs-12">
    @RenderBody()
</div>




@model Model.ViewModels.Judge.VmFinalGradeReportManagement

@{
    ViewBag.Title = "Final Scores Management";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<link href="~/Resources/CSS/grading.css" rel="stylesheet" />
<h2>Final Scores</h2>
<div>
    <button class="btn btn-primary" onclick="printDiv()">Print</button>
</div>
<div id="DivIdToPrint">
    <hr />
    @Html.Partial("_FinalGradesReportTable", Model.GradeReportList)
</div>
<div>
    <button class="btn btn-primary" onclick="printDiv()">Print</button>
</div>
<script>

    function printDiv() {

        $("#gradingEvaluationDetailModalContainer").hide();
        var divToPrint = document.getElementById('DivIdToPrint');

        var newWin = window.open('', 'Print-Window');

        newWin.document.open();

        newWin.document.write('<html><body onload="window.print()">' + divToPrint.innerHTML + '</body></html>');

        newWin.document.close();

        setTimeout(function () { newWin.close(); }, 10);
        $("#gradingEvaluationDetailModalContainer").show();

    }

</script>
